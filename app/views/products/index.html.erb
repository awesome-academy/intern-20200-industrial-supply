<%= provide :title, t(".all_products") %>
<%= render "header" %>
<div class="product spad">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12" id="list-products">
        <%= render "count" %>
        <div class="grid">
          <%= render @products %>
        </div>
        <%= paginate @products, theme: "twitter-bootstrap-4" %>
      </div>
    </div>
  </div>
</div>
